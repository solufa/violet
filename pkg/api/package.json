{
  "name": "@violet/api",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "migrate:dev": "pnpm run _:build && pnpm run _:migrate:dev",
    "migrate:reset": "pnpm run _:build && pnpm run _:migrate:reset",
    "start": "cross-env NODE_ENV=production node build/index.js",
    "prisma:prod": "pnpm run _:build && pnpm run _:prisma:prod",
    "prisma:seed": "pnpm run _:build && pnpm run _:prisma:seed",
    "prisma:seed:dev": "pnpm run _:build && pnpm run prisma:seed:dev",
    "prisma:format": "prisma format",
    "_:migrate:dev": "prisma migrate dev",
    "_:migrate:reset": "prisma migrate reset",
    "_:prisma:prod": "node ./prisma/build/index.js --prod",
    "_:prisma:seed": "node ./prisma/build/index.js --seed",
    "_:prisma:seed:dev": "pnpm run _:prisma:seed -- dev",
    "_:dev": "run-p -l _:build:* && run-p -l _:dev:*",
    "_:dev:build": "pnpm run migrate:dev && pnpm run _:prisma:seed:dev && run-p -l _:build-watch:*",
    "_:dev:serve": "node-dev --respawn build/index.js",
    "_:dev:aspida": "aspida --watch",
    "_:dev:frourio": "onchange -i -k -d 1000 './api/**/*' -e '**/$*.ts' -- frourio",
    "_:build:entries": "ts-node -T ./node_modules/@violet/scripts/build-files.ts --from-dir=./src/entries --to-dir=./build --target=node16",
    "_:build:prisma-index": "ts-node -T ./node_modules/@violet/scripts/build-file.ts --from=./prisma/scripts/index.ts --to=./prisma/build/index.js --target=node16",
    "_:build:prisma-seed": "ts-node -T ./node_modules/@violet/scripts/build-files.ts --from-dir=./prisma/scripts/seed --to-dir=./prisma/build/seed --target=node16",
    "_:build:prisma-prod": "ts-node -T ./node_modules/@violet/scripts/build-files.ts --from-dir=./prisma/scripts/prod --to-dir=./prisma/build/prod --target=node16 --clean",
    "_:build-prod:entries": "pnpm run _:build:entries -- --prod",
    "_:build-prod:prisma-index": "pnpm run _:build:prisma-index -- --prod",
    "_:build-prod:prisma-seed": "pnpm run _:build:prisma-seed -- --prod",
    "_:build-prod:prisma-prod": "pnpm run _:build:prisma-prod -- --prod",
    "_:build-watch:entries": "pnpm run _:build:entries -- --watch",
    "_:build-watch:prisma-index": "pnpm run _:build:prisma-index -- --watch",
    "_:build-watch:prisma-seed": "pnpm run _:build:prisma-seed -- --watch",
    "_:build-watch:prisma-prod": "pnpm run _:build:prisma-prod -- --watch",
    "_:generate": "prisma generate && frourio && aspida",
    "_:build": "run-p -l _:build-prod:*",
    "_:test": "jest",
    "_:typecheck": "tsc --noEmit"
  },
  "prisma": {
    "seed": "pnpm run prisma:seed:dev"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.41.0",
    "@aws-sdk/credential-providers": "^3.41.0",
    "@aws-sdk/s3-request-presigner": "^3.41.0",
    "@prisma/client": "3.13.0",
    "arg": "^5.0.1",
    "aspida": "^1.7.1",
    "class-validator": "^0.13.1",
    "dayjs": "^1.10.7",
    "dotenv": "^10.0.0",
    "fast-json-stringify": "^2.7.11",
    "fastify": "^3.20.2",
    "fastify-cookie": "^5.3.2",
    "fastify-cors": "^6.0.2",
    "fastify-csrf": "^3.1.0",
    "fastify-helmet": "^5.3.2",
    "fastify-multipart": "^4.0.7",
    "fastify-plugin": "^3.0.0",
    "firebase-admin": "^10.0.0",
    "google-auth-library": "^7.10.2",
    "http-errors": "^1.8.1",
    "prisma": "3.13.0",
    "source-map-support": "^0.5.20",
    "velona": "^0.7.0"
  },
  "devDependencies": {
    "@types/http-errors": "^1.8.1",
    "@violet/api": "workspace:*",
    "@violet/def": "workspace:*",
    "@violet/lib": "workspace:*",
    "@violet/scripts": "workspace:*"
  }
}
